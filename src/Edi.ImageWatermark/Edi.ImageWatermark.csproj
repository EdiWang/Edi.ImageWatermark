<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.1;net6.0</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Edi Wang</Authors>
		<Company>edi.wang</Company>
		<PackageProjectUrl>https://edi.wang</PackageProjectUrl>
		<RepositoryType></RepositoryType>
		<RepositoryUrl>https://github.com/EdiWang/Edi.ImageWatermark</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>2.5.0</Version>
		<FileVersion>2.5.0.0</FileVersion>
		<PackageTags>Watermark</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="SixLabors.ImageSharp" Version="1.0.4" />
		<PackageReference Include="SixLabors.ImageSharp.Drawing" Version="1.0.0-beta13" />
	</ItemGroup>

</Project>
