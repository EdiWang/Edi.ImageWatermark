<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>2.11.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>edi.wang</Company>
    <Authors>Edi Wang</Authors>
    <Description>Add watermark to images</Description>
    <PackageTags>Watermark</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://edi.wang</PackageProjectUrl>
    <RepositoryType></RepositoryType>
    <RepositoryUrl>https://github.com/EdiWang/Edi.ImageWatermark</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <TargetFrameworks>net6.0;net8.0</TargetFrameworks>
    <PackageIcon>edi-logo-blue.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SixLabors.ImageSharp.Drawing" Version="2.1.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath="\" />
    <None Include="..\..\img\edi-logo-blue.png" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>