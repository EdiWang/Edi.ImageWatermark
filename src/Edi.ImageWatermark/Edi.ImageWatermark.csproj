<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <Version>2.9.1</Version>
        <AssemblyName>Edi.ImageWatermark</AssemblyName>
        <RootNamespace>Edi.ImageWatermark</RootNamespace>
        <IsTestProject>false</IsTestProject>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <IsPackable>true</IsPackable>
        <ImplicitUsings>enable</ImplicitUsings>
        <Company>edi.wang</Company>
        <Product>ImageWatermark</Product>
        <Authors>Edi Wang</Authors>
        <Description>Nuget package of 'ImageWatermark' provided by Edi</Description>
        <PackageId>Edi.ImageWatermark</PackageId>
        <PackageTags>Watermark</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://edi.wang</PackageProjectUrl>
        <RepositoryType></RepositoryType>
        <RepositoryUrl>https://github.com/EdiWang/Edi.ImageWatermark</RepositoryUrl>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <FileVersion>2.9.0.0</FileVersion>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="SixLabors.ImageSharp.Drawing" Version="1.0.0" />
    </ItemGroup>
</Project>